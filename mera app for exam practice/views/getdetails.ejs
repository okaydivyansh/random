<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table{
            border: 2px solid black;
            
        }

    </style>
</head>
<body>
    <table>
        <tr>
            <th>order number</th>
            <th>customer number</th>
            <th>items</th>
            <th>amount</th>
        </tr>
    <% details.forEach(function(item){ %>
        <tr>
         <td><%= item.order%></td>
         <td><%= item.customer %></td>
        <td><%= item.items%></td>
         <td><%= item.amount %></td>
         </tr>
        <% }) %>
    </table>
</body>
</html>